{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "jsx": "react-native",
      "paths": {
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "react-native-draggable-flatlist"
      ]
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      ".expo/types/**/*.ts",
      "expo-env.d.ts"
    ]
  }
  